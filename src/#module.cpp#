
#include "module.hpp"

namespace ASV {

Module::Module() {
  time = 0;
  ticks = 0;
}

Module::~Module() {

}

// # Tick

bool Module::tick() {
  ticks += 1;
  time += ModuleTimestep;
}

bool Module::tick(double step) {
  for(; step > 0; step -= ModuleTimestep) {
    tick();
  }
}

double Module::getTime(void) {
  return time;
}

long int Module::getTicks(void) {
  return ticks;
}

}

